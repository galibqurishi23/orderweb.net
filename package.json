{
  "name": "orderweb-restaurant-system",
  "version": "1.0.0",
  "description": "Complete restaurant management system with online ordering",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9010 -H 0.0.0.0",
    "dev:fast": "NODE_ENV=development next dev -p 9010 -H 0.0.0.0 --experimental-https",
    "dev:turbo": "next dev -p 9010 -H 0.0.0.0 --turbo",
