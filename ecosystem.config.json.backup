{
  "apps": [{
    "name": "orderweb-restaurant",
    "script": "npm",
    "args": "start",
    "instances": 1,
    "exec_mode": "cluster",
    "env": {
      "NODE_ENV": "production",
      "PORT": "9002"
    },
    "error_file": "./logs/err.log",
    "out_file": "./logs/out.log",
    "log_file": "./logs/combined.log",
    "time": true,
    "max_restarts": 10,
    "min_uptime": "10s",
    "max_memory_restart": "1G"
  }]
}
